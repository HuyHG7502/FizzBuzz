{
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "enable": true,
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "trailingComma": "es5",
    "useEditorConfig": true,
    "plugins": ["prettier-plugin-tailwindcss"]
  }
  